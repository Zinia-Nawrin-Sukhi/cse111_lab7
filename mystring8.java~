class mystring8{
   char[]c;
  
  mystring8(String s1){
    c=s1.toCharArray();
  }
  public String tolowercase(){
    char[]c2=new char[c.length];
    for(int i=0;i<c.length;i++){
      c2[i]=c[i];
    }
    for(int i=0;i<c.length;i++){
      if(c2[i]>='A' && c2[i]<='Z'){
         c2[i]=c2[i]+32;
      }
    }
    return String.valueof(c2);
  }
}