class mystring9{
   char[]c;
  
  mystrin9(String s1){
    c=s1.toCharArray();
  }
  public String touppercase(){
    char[]c2=new char[c.length];
    int k=0;
    for(int i=0;i<c.length;i++){
      c2[i]=c[i];
    }
    for(int i=0;i<c.length;i++){
      if(c2[i]>='a' && c2[i]<='z'){
         k=(int)c2[i]-32;
         c2[i]=(char)k;
      }
    }
    return String.valueOf(c2);
  }
}